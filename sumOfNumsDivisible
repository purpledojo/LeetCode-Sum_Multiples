class Solution(object):
    def sumOfMultiples(self, n):
        """
        :type n: int
        :rtype: int
        """
        add = 0
        for x in range (1,n+1):
            if x % 3 == 0:
                add = add + int(x)
            elif x % 5 ==0:
                add = add + int(x)
            elif x % 7==0:
                add = add + int(x)
            print(add)
        return (add)
